{
  "name": "teapie-extensions",
  "displayName": "TeaPie Extensions",
  "description": "Run TeaPie tests directly from VS Code",
  "version": "0.0.13",
  "publisher": "kros",
  "icon": "resources/teapie.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kros-sk/TeaPie.Extensions"
  },
  "bugs": {
    "url": "https://github.com/Kros-sk/TeaPie.Extensions/issues"
  },
  "homepage": "https://github.com/Kros-sk/TeaPie.Extensions#readme",
  "galleryBanner": {
    "color": "#C80000",
    "theme": "dark"
  },
  "keywords": [
    "teapie",
    "testing",
    "api",
    "http",
    "rest",
    "automation"
  ],
  "author": {
    "name": "Kros a.s.",
    "url": "https://www.kros.sk/"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "workspaceContains:**/*.http",
    "onView:teapieExplorer",
    "onCommand:teapie-extensions.runDirectory",
    "onCommand:teapie-extensions.runFile",
    "onCommand:teapie-extensions.cycleTestFiles",
    "onCommand:teapie-extensions.nextTestCase",
    "onCommand:teapie-extensions.nextTestCaseWithSubdirs",
    "onCommand:teapie-extensions.generateTestCase",
    "onCommand:teapie-extensions.refreshExplorer",
    "onCommand:teapie-extensions.openFile",
    "onCommand:teapie-extensions.shiftSubsequentTests",
    "onCommand:teapie-extensions.previewHttpFile",
    "onCommand:teapie-extensions.runHttpTest",
    "onCommand:teapie.reload",
    "onCommand:teapie.run",
    "onCommand:teapie.runInTerminal",
    "onCommand:teapie.reloadDocs",
    "onCommand:teapie.setupCsxSupport",
    "onCommand:teapie.shiftTestNames"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "teapie-explorer",
          "title": "TeaPie Explorer",
          "icon": "resources/teapie.png"
        }
      ]
    },
    "views": {
      "teapie-explorer": [
        {
          "id": "teapieExplorer",
          "name": "Test Cases"
        }
      ]
    },
    "commands": [
      {
        "command": "teapie-extensions.runDirectory",
        "title": "TeaPie: Run Tests",
        "icon": "$(play)"
      },
      {
        "command": "teapie-extensions.runFile",
        "title": "TeaPie: Run Test",
        "icon": "$(play)"
      },
      {
        "command": "teapie-extensions.cycleTestFiles",
        "title": "TeaPie: Cycle Test Files"
      },
      {
        "command": "teapie-extensions.nextTestCase",
        "title": "TeaPie: Next Test Case"
      },
      {
        "command": "teapie-extensions.nextTestCaseWithSubdirs",
        "title": "TeaPie: Next Test Case (Include Subdirectories)"
      },
      {
        "command": "teapie-extensions.generateTestCase",
        "title": "TeaPie: Generate New Test Case"
      },
      {
        "command": "teapie-extensions.exploreCollection",
        "title": "TeaPie: Explore Collection"
      },
      {
        "command": "teapie-extensions.refreshExplorer",
        "title": "TeaPie: Refresh Explorer"
      },
      {
        "command": "teapie-extensions.openFile",
        "title": "Open File",
        "icon": "$(go-to-file)"
      },
      {
        "command": "teapie-extensions.previewHttpFile",
        "title": "Preview HTTP",
        "icon": "$(preview)"
      },
      {
        "command": "teapie.reload",
        "title": "TeaPie: Reload Collection"
      },
      {
        "command": "teapie.run",
        "title": "TeaPie: Run test case"
      },
      {
        "command": "teapie.runInTerminal",
        "title": "TeaPie: Run test case in terminal"
      },
      {
        "command": "teapie.reloadDocs",
        "title": "TeaPie: Reload Documentation"
      },
      {
        "command": "teapie.setupCsxSupport",
        "title": "TeaPie: Setup C# Script Support"
      },
      {
        "command": "teapie.shiftTestNames",
        "title": "TeaPie: Shift Test Names",
        "category": "TeaPie"
      },
      {
        "command": "teapie-extensions.shiftSubsequentTests",
        "title": "TeaPie: Shift Subsequent Tests",
        "category": "TeaPie"
      },
      {
        "command": "teapie-extensions.runHttpTest",
        "title": "Run test (F5)",
        "icon": "$(play)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "teapie-extensions.refreshExplorer",
          "when": "view == teapieExplorer",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "teapie-extensions.openFile",
          "when": "view == teapieExplorer && (viewItem == httpFile || viewItem == initFile || viewItem == testFile || viewItem == testCase)",
          "group": "inline"
        },
        {
          "command": "teapie-extensions.runFile",
          "when": "view == teapieExplorer && (viewItem == httpFile || viewItem == testCase)",
          "group": "inline"
        },
        {
          "command": "teapie-extensions.runDirectory",
          "when": "view == teapieExplorer && viewItem == directory",
          "group": "inline"
        },
        {
          "command": "teapie-extensions.shiftSubsequentTests",
          "when": "view == teapieExplorer && (viewItem == httpFile || viewItem == testCase || viewItem == directory)"
        }
      ],
      "explorer/context": [
        {
          "command": "teapie-extensions.runDirectory",
          "group": "teapie@1",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "teapie-extensions.runFile",
          "group": "teapie@2",
          "when": "resourceFilename =~ /.*-req\\.http$/ || resourceFilename =~ /.*-test\\.csx$/"
        },
        {
          "command": "teapie-extensions.generateTestCase",
          "group": "teapie@4",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "teapie-extensions.exploreCollection",
          "group": "teapie@5",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "teapie-extensions.previewHttpFile",
          "group": "teapie@6",
          "when": "resourceExtname == .http"
        },
        {
          "command": "teapie-extensions.shiftSubsequentTests",
          "title": "TeaPie: Shift Subsequent Tests",
          "group": "teapie@1",
          "when": "resourceFilename =~ /.*-req\\.http$/ || resourceFilename =~ /.*-test\\.csx$/ || explorerResourceIsFolder"
        }
      ],
      "editor/context": [
        {
          "command": "teapie-extensions.runFile",
          "when": "resourceFilename =~ /\\.(csx|http)$/",
          "group": "navigation@1"
        },
        {
          "command": "teapie-extensions.previewHttpFile",
          "when": "resourceExtname == .http",
          "group": "navigation@3"
        }
      ],
      "editor/title": [
        {
          "command": "teapie-extensions.runHttpTest",
          "when": "resourceExtname == .http",
          "group": "navigation"
        },
        {
          "command": "teapie-extensions.previewHttpFile",
          "when": "resourceExtname == .http",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "command": "teapie-extensions.runFile",
        "key": "f5",
        "mac": "f5",
        "when": "editorTextFocus && (resourceFilename =~ /.*-req\\.http$/ || (resourceFilename =~ /.*-test\\.csx$/ && resourcePath =~ /.*-req\\.http$/))"
      },
      {
        "command": "teapie-extensions.previewHttpFile",
        "key": "f6",
        "mac": "f6",
        "when": "editorTextFocus && resourceFilename =~ /.*-req\\.http$/"
      },
      {
        "command": "teapie-extensions.cycleTestFiles",
        "key": "f7",
        "mac": "f7"
      },
      {
        "command": "teapie-extensions.nextTestCase",
        "key": "alt+f7",
        "mac": "alt+f7"
      },
      {
        "command": "teapie-extensions.nextTestCaseWithSubdirs",
        "key": "ctrl+alt+f7",
        "mac": "cmd+alt+f7"
      }
    ],
    "languages": [
      {
        "id": "http",
        "extensions": [".http"],
        "aliases": ["HTTP", "http"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "http",
        "scopeName": "source.http",
        "path": "./syntaxes/http.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "csharp",
        "path": "./snippets/csharp.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/xml2js": "^0.4.14",
    "acorn": "^8.14.1",
    "acorn-jsx": "^5.3.2",
    "ajv": "^6.12.6",
    "ansi-regex": "^5.0.1",
    "ansi-styles": "^4.3.0",
    "argparse": "^2.0.1",
    "array-union": "^2.1.0",
    "balanced-match": "^1.0.2",
    "brace-expansion": "^2.0.1",
    "braces": "^3.0.3",
    "callsites": "^3.1.0",
    "chalk": "^4.1.2",
    "color-convert": "^2.0.1",
    "color-name": "^1.1.4",
    "concat-map": "^0.0.1",
    "cross-spawn": "^7.0.6",
    "debug": "^4.4.0",
    "deep-is": "^0.1.4",
    "dir-glob": "^3.0.1",
    "doctrine": "^3.0.0",
    "escape-string-regexp": "^4.0.0",
    "eslint-scope": "^7.2.2",
    "eslint-visitor-keys": "^3.4.3",
    "espree": "^9.6.1",
    "esquery": "^1.6.0",
    "esrecurse": "^4.3.0",
    "estraverse": "^5.3.0",
    "esutils": "^2.0.3",
    "fast-deep-equal": "^3.1.3",
    "fast-glob": "^3.3.3",
    "fast-json-stable-stringify": "^2.1.0",
    "fast-levenshtein": "^2.0.6",
    "fastq": "^1.19.1",
    "file-entry-cache": "^6.0.1",
    "fill-range": "^7.1.1",
    "find-up": "^5.0.0",
    "flat-cache": "^3.2.0",
    "flatted": "^3.3.3",
    "fs.realpath": "^1.0.0",
    "glob": "^7.2.3",
    "glob-parent": "^6.0.2",
    "globals": "^13.24.0",
    "globby": "^11.1.0",
    "graphemer": "^1.4.0",
    "has-flag": "^4.0.0",
    "ignore": "^5.3.2",
    "import-fresh": "^3.3.1",
    "imurmurhash": "^0.1.4",
    "inflight": "^1.0.6",
    "inherits": "^2.0.4",
    "is-extglob": "^2.1.1",
    "is-glob": "^4.0.3",
    "is-number": "^7.0.0",
    "is-path-inside": "^3.0.3",
    "isexe": "^2.0.0",
    "js-yaml": "^4.1.0",
    "json-buffer": "^3.0.1",
    "json-schema-traverse": "^0.4.1",
    "json-stable-stringify-without-jsonify": "^1.0.1",
    "keyv": "^4.5.4",
    "levn": "^0.4.1",
    "locate-path": "^6.0.0",
    "lodash.merge": "^4.6.2",
    "merge2": "^1.4.1",
    "micromatch": "^4.0.8",
    "minimatch": "^9.0.3",
